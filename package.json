{
  "name": "gogogo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build && node deploy.js",
    "train": "node runSelfPlayTraining.js",
    "demo": "node demoAgenticLoop.js",
    "blog": "npx http-server blog -p 3001 -o",
    "visualize": "tsx src/cli/visualize.ts",
    "viz-python": "python3 scripts/visualize_policy.py",
    "watch": "npx tsx watchGame.js",
    "play": "npx tsx playGame.js"
  },
  "dependencies": {
    "beautiful-skill-tree": "^1.7.1",
    "onnxruntime-web": "^1.23.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^3.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^26.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^3.0.0",
    "vitest-localstorage-mock": "^0.1.2"
  },
  "description": "GoGoGo - An idle/incremental game of Go",
  "keywords": [
    "go",
    "baduk",
    "weiqi",
    "idle game",
    "incremental"
  ],
  "author": "",
  "license": "MIT"
}
